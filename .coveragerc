[run]
branch = True
source = .
omit =
    .tox/*
    setup.py
    # Don't complain if non-runnable code isn't run
    */__main__.py

[report]
exclude_lines =
    # Re-enable the standard pragma
    \#\s*pragma: no cover

    # Don't complain if non-runnable code isn't run
    ^if __name__ == ['"]__main__['"]:$

